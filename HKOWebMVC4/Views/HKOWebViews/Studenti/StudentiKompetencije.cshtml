@model IEnumerable<HKOWebMVC4.KolegijKompetencije_Result>

<h2>Kompetencije studenta</h2>

<table class="table" id="studentKompetencije">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Kompetencija)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Kompetencija)
                </td>
            </tr>
        }
    </tbody>
</table>
