@model IEnumerable<HKOWebMVC4.ZanimanjeRadnaMjesta_Result>

@{
    ViewBag.Title = "RadnaMjesta";
}

<h2>RadnaMjesta</h2>

<table id="radnaMjesta">
    <thead>
        <tr>
            <th>
                Radno mjesto
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.RadnoMjesto)
                </td>
            </tr>
        }
    </tbody>

</table>

@Html.ActionLink("Povratak", "Index", new { controller = "BuduciStudent", @class = "btn btn-success" })